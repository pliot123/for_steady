#include<stdio.h>
#include<iostream>

using namespace std;

int main(void)
{
	int a = 300;
	int b = 60;
	int c = 10;
	int ac = 0;
	int bc = 0;
	int cc = 0;

	int time;
	cin >> time;


	while (time >= a)
	{
			time -= a;
			ac++;
	}

	while (time >= b)
	{
			time -= b;
			bc++;
	}
	while (time >= c)
	{
			time -= c;
			cc++;
	}
	

	if (time == 0)
	{
		printf("%d %d %d", ac, bc, cc);
	}
	else
	{
		printf("-1");
	}
}
